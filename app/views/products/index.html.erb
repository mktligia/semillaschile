<h1>Lista de Productos</h1>
<div class="container">
  <div class="row justify-content-center ">
  
    <table>
      <thead>
        <tr>
          <th>Id</th>
          <th>Nombre</th>
          <th>Descripcion</th>
          <th>Lugar</th>
          <th>Pa√¨s</th>
          <th>Precio</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @products.each do |item| %>
          <tr>
            <td><%= item.id %></td>
            <td><%= item.name %></td>
            <td><%= item.content %></td>
            <td><%= item.address %></td>
            <td><%= item.country %></td>
            <td><%= item.price_cents %></td>
            </tr>
        <% end %>
      </tbody>
    </table>
  </div>


</div>
  



